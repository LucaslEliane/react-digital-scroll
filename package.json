{
  "name": "digital-scroll",
  "version": "0.1.0",
  "description": "数字滚动",
  "author": "陆鑫 <luxin1@corp.netease.com>",
  "aliasName": {
    "zn": "数字滚动",
    "en": "digital-scroll"
  },
  "groupName": "其他",
  "category": "base",
  "business": "platform",
  "keywords": [
    "scroll",
    "react",
    "digital"
  ],
  "scripts": {
    "dev": "webpack-dev-server --port 7909 --host 0.0.0.0 --content-base demo/ --config demo/webpack.config.js --progress --hot --inline --disableHostCheck",
    "build:demo": "webpack -p --config demo/webpack.config.js --progress --env=production",
    "js:commonjs": "cross-env BABEL_ENV=commonjs babel src/ --out-dir dist/",
    "js:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "test": "jest",
    "css:copy": "copyfiles -f ./src/*.less ./dist",
    "css": "lessc ./src/index.less ./dist/index.css --autoprefix=\"IOS >= 8.0, Android >= 4.0\" --inline-urls",
    "build": "npm run test && npm run build:demo && npm run js:commonjs && npm run js:es && npm run css && npm run css:copy"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.0.5",
    "cryptiles": "^4.1.3",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "hoek": "^6.1.2",
    "jest": "^22.2.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-inline-urls": "^1.2.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^3.1.11",
    "yargs": "^9.0.1"
  },
  "peerDependencies": {
    "react": ">=16.4.0",
    "react-dom": ">=16.4.0",
    "prop-types": ">=15.3.0",
    "babel-plugin-transform-runtime": ">=6.23.0"
  },
  "engines": {
    "node": ">= 6.2.0",
    "npm": ">= 3.0.0"
  },
  "jsnext:main": "es/index.js",
  "main": "dist/index.js",
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testMatch": [
      "<rootDir>/test/*.js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {},
  "directories": {
    "test": "test"
  },
  "license": "ISC"
}
