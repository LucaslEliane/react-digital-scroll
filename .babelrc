{
    "env": {
      "commonjs": {
        "presets": [
          ["env", {
            "modules" : "commonjs",
            "loose": true
          }],
          "stage-0",
          "react"
        ],
        "plugins": [
          "add-module-exports"
        ]
      },
      "es": {
        "presets": [
          ["env", {
            "modules" : false,
            "loose": true
          }],
          "stage-0",
          "react"
        ]
      },
      "test": {
        "presets": [
          ["env", {
            "modules" : "commonjs",
            "loose": true
          }],
          "stage-0",
          "react"
        ]
      }
    }
  }
  